{
	"extends": "./jsconfig.paths.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@": [
				"./src"
			],
			"@*": [
				"./src/*"
			],
			"@page": [
				"src/page"
			],
			"@asset": [
				"./src/asset"
			],
			"@font": [
				"./src/asset/font"
			],
			"@image": [
				"./src/asset/image"
			],
			"@style": [
				"./src/asset/style"
			]
		}
	}
}